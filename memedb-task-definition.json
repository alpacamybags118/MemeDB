{
  "family": "console-sample-app-static",
  "containerDefinitions": [
    {
      "image": "%REPOSITORY_URL%:v_%BUILD_NUMBER%",
      "name": "sample-app",
      "cpu": 10,
      "memory": 300,
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80,
          "protocol": "tcp"
        }
      ],
    }
  ],
}
